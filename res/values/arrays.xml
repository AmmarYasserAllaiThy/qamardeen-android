    </string-array>
    
    <!-- map "prayer_options" to corresponding values
         can use for male and female since the excused
         option is the last option, so it will be ignored
         for the male case since the corresponding strings
         array will contain one element less than the values
         array -->
    <integer-array name="prayer_values">
        <item>4</item>  <!-- jama3a with sunnah -->
        <item>3</item>  <!-- jama3a -->
        <item>2</item>  <!-- alone with sunnah -->
        <item>1</item>  <!-- alone -->
        <item>5</item>  <!-- late -->
        <item>0</item>  <!-- not set -->
        <item>6</item>  <!-- excused -->
    </integer-array>
       
